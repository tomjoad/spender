<!DOCTYPE html>
<html>
  <head>
    <title>Spender</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <section id="main">
      <div class="container">
        <%= yield %>
      </div>
      <div class="footer">
        <%= render 'layouts/partials/footer'%>
      </div>
    </section>
  </body>
</html>
